
<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="commandSet_en-us">
    <AppName>InsideUWP</AppName>

    <CommandPrefix>InsideUWP</CommandPrefix> <!-- change this -->

    <Example>Go to ...</Example>



    <Command Name="InsideUWP">

      <Example>Go to ...</Example>

      <ListenFor>Go to {options}</ListenFor>

      <Feedback>Navigating to {options}</Feedback>

      <Navigate/>

    </Command>



    <Command Name="search">

      <Example>Search</Example>

      <ListenFor>Search [for] {searchTerm}</ListenFor>

      <Feedback>Searching for {searchTerm}</Feedback>

      <Navigate/>

    </Command>



    <PhraseList Label="options">
      <Item> Windows 10 </Item>
      <Item> Windows 10 Mobile </Item>
      <Item> Reviews </Item>
      <Item> App Mondays </Item>
    </PhraseList>



    <PhraseTopic Label="searchTerm" Scenario="Search">

      <Subject>Search Terms</Subject>

    </PhraseTopic>



  </CommandSet>

</VoiceCommands>
